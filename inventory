package com.company;

import java.util.ArrayList;


    public class inventory {
        private ArrayList<purchase> inventory = new ArrayList<purchase>();

        public ArrayList<purchase> getInventory() {
            return inventory;
        }

        public void setInventory(ArrayList<purchase> inventory) {
            this.inventory = inventory;
        }

        public void addPurchase(purchase purchase) {
            inventory.add(purchase);
        }
    }

